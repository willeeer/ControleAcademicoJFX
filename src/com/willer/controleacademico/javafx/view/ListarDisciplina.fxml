<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ButtonBar?>
<?import javafx.scene.control.ScrollPane?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane prefHeight="456.0" prefWidth="700.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1"
            fx:controller="com.willer.controleacademico.javafx.controllers.disciplina.ListarDisciplinaController">
    <children>
        <ScrollPane prefHeight="400.0" prefWidth="700.0">
            <content>
                <AnchorPane minHeight="0.0" minWidth="0.0">
                    <children>
                        <TableView fx:id="tableData">
                            <columns>
                                <TableColumn fx:id="nome" prefWidth="245.0" text="Nome"/>
                                <TableColumn fx:id="codigo" minWidth="0.0" prefWidth="105.0" text="Código"/>
                                <TableColumn fx:id="credito" minWidth="0.0" text="Créditos"/>
                                <TableColumn fx:id="carga" minWidth="0.0" prefWidth="116.0" text="Carga Horária"/>
                                <TableColumn fx:id="periodo" minWidth="0.0" prefWidth="129.0" text="Período"/>
                            </columns>
                        </TableView>
                    </children>
                </AnchorPane>
            </content>
        </ScrollPane>
        <ButtonBar layoutX="159.0" layoutY="400.0" prefHeight="40.0" prefWidth="200.0"/>
        <Button fx:id="voltarButton" contentDisplay="CENTER" layoutX="324.0" layoutY="415.0" mnemonicParsing="false"
                onAction="#acaoBotaoVoltar" prefHeight="25.0" prefWidth="126.0" text="Voltar"/>
    </children>
</AnchorPane>
